{
  "name": "population-cache-warmer",
  "main": "index.ts",
  "compatibility_date": "2025-03-01",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "triggers": {
    "crons": [
      // 毎週月曜午前3時（JST）に実行 = UTC 日曜 18:00
      "0 18 * * SUN"
    ]
  },
  "kv_namespaces": [
    {
      "binding": "POPULATION_CACHE",
      "id": "9e774831fd5f40bab2eed413502f79f8"
    }
  ],
  "observability": {
    "logs": {
      "enabled": true,
      "head_sampling_rate": 1,
      "invocation_logs": true
    }
  }
}
